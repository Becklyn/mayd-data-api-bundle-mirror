{
    "name": "mayd/data-api-bundle",
    "description": "Bundle for interacting with the mayd data API",
    "license": "proprietary",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": {"Mayd\\DataApiBundle\\": ""}
    },
    "autoload-dev": {
        "psr-4": {"Tests\\Mayd\\DataApiBundle\\": "tests/"}
    },
    "config": {
        "sort-packages": true
    }
}
